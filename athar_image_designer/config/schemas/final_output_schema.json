{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Athar Workflow Output",
  "type": "object",
  "required": ["theme", "prompt_used", "image_url", "gdrive_url", "seed", "aspect_ratio"],
  "properties": {
    "theme": {"type": "string"},
    "prompt_used": {"type": "string"},
    "image_url": {"type": "string", "format": "uri"},
    "gdrive_url": {"type": "string", "format": "uri"},
    "seed": {"type": "string"},
    "aspect_ratio": {"type": "string", "pattern": "^\\d+:\\d+$"}
  }
}
